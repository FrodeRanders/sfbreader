@prefix def:  <http://data.sfa.se/ontologi/lagrum/definition#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix eli: <http://data.europa.eu/eli/ontology#> .
@prefix eu: <http://publications.europa.eu/resource/authority/subdivision/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

#
# Försäkringskassans ontologi for svensk lagtext.
#
# Modellprincip:
# - ELI utgör ryggraden för juridisk resurs och del-helhet-relationer.
# - EU:s subdivision-kodlista anger klassificering av strukturell nivå.
#
# Nivåer enligt autority/subdivision
# +======================================================================+
# |      Nivå      | SKOS:Concept |  Svensk term  |    Engelsk term      |
# +======================================================================+
# |      Lag       |   eu:ACT     |     akt       |        act           |
# +----------------+--------------+---------------+----------------------+
# |   Avdelning    |   eu:PRT     |     del       |        part          |
# +----------------+--------------+---------------+----------------------+
# | Underavdelning |   eu:TIS     |   avdelning   |  title (subdivision) |
# +----------------+--------------+---------------+----------------------+
# |    Kapitel     |   eu:CPT     |    kapitel    |       chapter        |
# +----------------+--------------+---------------+----------------------+
# |   Paragraf     |   eu:ART     |    artikel    |       article        |
# +----------------+--------------+---------------+----------------------+
# |    Stycke      |   eu:PAR     |    punkt      |      paragraph       |
# +----------------+--------------+---------------+----------------------+
# |    Punkt       |   eu:SUB     |    stycke     |    subparagraph      |
# +----------------+--------------+---------------+----------------------+
# | (1), (2), ...  |   eu:PNT     |               |                      |
# | (a), (b), ...  |   eu:PTA     |     led       |       point          |
# | (i), (ii), ... |   eu:PTI     |               |                      |
# +----------------+--------------+---------------+----------------------+
#
# Kommentar:
#     Den engelska definitionen av eu:PAR svarar mot stycke medan den
#     engelska definitionen av eu:SUB svarar mot punkt. Den svenska termen
#     ser ut att vara omkastad, så vi utgår därför ifrån definitionen
#     i stället för svensk term.
#     Vi har en liknande situation för eu:PRT <-> eu:TIS, där den svenska
#     termen är märklig. Definitionen av eu:TIS nämner att denna "faller
#     mellan eu:PRT och eu:CPT", så detta måste vara 'underavdelning'.
#


def: a owl:Ontology ;
    rdfs:label "Forsakringskassans ontologi for svensk lagtext"@sv ;
    rdfs:comment "Generell ontologi for svensk lagtext, profilerad med ELI och EU subdivision-koder."@sv .


def:Lag a owl:Class ;
    rdfs:label "Lag"@sv ;
    rdfs:comment "En svensk forfattning som juridisk resurs."@sv ;
    rdfs:subClassOf eli:LegalResource .


def:StrukturellEnhet a owl:Class ;
    rdfs:label "Strukturell enhet"@sv ;
    rdfs:comment "Strukturell enhet i en lagtext, t.ex. kapitel eller paragraf."@sv ;
    rdfs:subClassOf eli:LegalResourceSubdivision .


def:Avdelning a owl:Class ;
    rdfs:label "Avdelning"@sv ;
    rdfs:subClassOf def:StrukturellEnhet ,
        [ a owl:Restriction ; owl:onProperty def:subdivisionCode ; owl:hasValue eu:PRT ] .


def:Underavdelning a owl:Class ;
    rdfs:label "Underavdelning"@sv ;
    rdfs:comment "Niva mellan avdelning och kapitel (nar den finns)."@sv ;
    rdfs:subClassOf def:StrukturellEnhet ,
        [ a owl:Restriction ; owl:onProperty def:subdivisionCode ; owl:hasValue eu:TIS ] .


def:Kapitel a owl:Class ;
    rdfs:label "Kapitel"@sv ;
    rdfs:subClassOf def:StrukturellEnhet ,
        [ a owl:Restriction ; owl:onProperty def:subdivisionCode ; owl:hasValue eu:CPT ] .


def:Paragraf a owl:Class ;
    rdfs:label "Paragraf"@sv ;
    rdfs:subClassOf def:StrukturellEnhet ,
        [ a owl:Restriction ; owl:onProperty def:subdivisionCode ; owl:hasValue eu:ART ] .


def:Stycke a owl:Class ;
    rdfs:label "Stycke"@sv ;
    rdfs:subClassOf def:StrukturellEnhet ,
        [ a owl:Restriction ; owl:onProperty def:subdivisionCode ; owl:hasValue eu:PAR ] .


def:Punkt a owl:Class ;
    rdfs:label "Punkt"@sv ;
    rdfs:subClassOf def:StrukturellEnhet ,
        [ a owl:Restriction ; owl:onProperty def:subdivisionCode ; owl:hasValue eu:SUB ] .


# Klassificering mot EU-kodlistan (SKOS-koncept).
def:subdivisionCode a owl:ObjectProperty ;
    rdfs:label "subdivision-kod"@sv ;
    rdfs:comment "Kod i EU:s authority/subdivision for strukturell niva."@sv ;
    rdfs:domain def:StrukturellEnhet ;
    rdfs:range skos:Concept ;
    rdfs:subPropertyOf dct:type .


# Del-helhet

def:harAvdelning a owl:ObjectProperty ;
    rdfs:label "har avdelning"@sv ;
    rdfs:domain def:Lag ;
    rdfs:range def:Avdelning ;
    rdfs:subPropertyOf eli:has_part .


def:ingarILag a owl:ObjectProperty ;
    rdfs:label "ingar i lag"@sv ;
    rdfs:domain def:Avdelning ;
    rdfs:range def:Lag ;
    rdfs:subPropertyOf eli:is_part_of ;
    owl:inverseOf def:harAvdelning .


def:harUnderavdelning a owl:ObjectProperty ;
    rdfs:label "har underavdelning"@sv ;
    rdfs:domain def:Avdelning ;
    rdfs:range def:Underavdelning ;
    rdfs:subPropertyOf eli:has_part .


def:ingarIAvdelning a owl:ObjectProperty ;
    rdfs:label "ingar i avdelning"@sv ;
    rdfs:domain def:Underavdelning ;
    rdfs:range def:Avdelning ;
    rdfs:subPropertyOf eli:is_part_of ;
    owl:inverseOf def:harUnderavdelning .


def:harKapitel a owl:ObjectProperty ;
    rdfs:label "har kapitel"@sv ;
    rdfs:domain def:Avdelning ;
    rdfs:range def:Kapitel ;
    rdfs:subPropertyOf eli:has_part .


def:ingarIAvdelningKapitel a owl:ObjectProperty ;
    rdfs:label "kapitel ingar i avdelning"@sv ;
    rdfs:domain def:Kapitel ;
    rdfs:range def:Avdelning ;
    rdfs:subPropertyOf eli:is_part_of ;
    owl:inverseOf def:harKapitel .


def:harKapitelDirekt a owl:ObjectProperty ;
    rdfs:label "har kapitel direkt"@sv ;
    rdfs:comment "For lagar dar kapitel ligger direkt under lag."@sv ;
    rdfs:domain def:Lag ;
    rdfs:range def:Kapitel ;
    rdfs:subPropertyOf eli:has_part .


def:ingarILagKapitel a owl:ObjectProperty ;
    rdfs:label "kapitel ingar i lag"@sv ;
    rdfs:domain def:Kapitel ;
    rdfs:range def:Lag ;
    rdfs:subPropertyOf eli:is_part_of ;
    owl:inverseOf def:harKapitelDirekt .


def:harParagraf a owl:ObjectProperty ;
    rdfs:label "har paragraf"@sv ;
    rdfs:domain def:Kapitel ;
    rdfs:range def:Paragraf ;
    rdfs:subPropertyOf eli:has_part .


def:ingarIKapitel a owl:ObjectProperty ;
    rdfs:label "paragraf ingar i kapitel"@sv ;
    rdfs:domain def:Paragraf ;
    rdfs:range def:Kapitel ;
    rdfs:subPropertyOf eli:is_part_of ;
    owl:inverseOf def:harParagraf .


def:harParagrafDirekt a owl:ObjectProperty ;
    rdfs:label "har paragraf direkt"@sv ;
    rdfs:comment "For lagar dar paragrafer ligger direkt under lag."@sv ;
    rdfs:domain def:Lag ;
    rdfs:range def:Paragraf ;
    rdfs:subPropertyOf eli:has_part .


def:ingarILagParagraf a owl:ObjectProperty ;
    rdfs:label "paragraf ingar i lag"@sv ;
    rdfs:domain def:Paragraf ;
    rdfs:range def:Lag ;
    rdfs:subPropertyOf eli:is_part_of ;
    owl:inverseOf def:harParagrafDirekt .


def:harStycke a owl:ObjectProperty ;
    rdfs:label "har stycke"@sv ;
    rdfs:domain def:Paragraf ;
    rdfs:range def:Stycke ;
    rdfs:subPropertyOf eli:has_part .


def:ingarIParagraf a owl:ObjectProperty ;
    rdfs:label "stycke ingar i paragraf"@sv ;
    rdfs:domain def:Stycke ;
    rdfs:range def:Paragraf ;
    rdfs:subPropertyOf eli:is_part_of ;
    owl:inverseOf def:harStycke .


def:harPunkt a owl:ObjectProperty ;
    rdfs:label "har punkt"@sv ;
    rdfs:domain def:Stycke ;
    rdfs:range def:Punkt ;
    rdfs:subPropertyOf eli:has_part .


def:ingarIStycke a owl:ObjectProperty ;
    rdfs:label "punkt ingar i stycke"@sv ;
    rdfs:domain def:Punkt ;
    rdfs:range def:Stycke ;
    rdfs:subPropertyOf eli:is_part_of ;
    owl:inverseOf def:harPunkt .


# Metadata

def:harTitel a owl:DatatypeProperty ;
    rdfs:label "har titel"@sv ;
    rdfs:domain def:StrukturellEnhet ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf dct:title .


def:harText a owl:DatatypeProperty ;
    rdfs:label "har text"@sv ;
    rdfs:domain def:Punkt ;
    rdfs:range xsd:string .


def:giltigFrom a owl:DatatypeProperty ;
    rdfs:label "giltig from"@sv ;
    rdfs:domain def:StrukturellEnhet ;
    rdfs:range xsd:date ;
    rdfs:subPropertyOf eli:first_date_entry_in_force .


def:giltigTill a owl:DatatypeProperty ;
    rdfs:label "giltig till"@sv ;
    rdfs:domain def:StrukturellEnhet ;
    rdfs:range xsd:date .
