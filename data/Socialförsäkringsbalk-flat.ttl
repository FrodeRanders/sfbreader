inst:Punkt_inst:Stycke_inst:Paragraf_A_1_3_base_3_1 a def:Punkt ;
    def:harText "Vissa gemensamma bestämmelser om förmånerna, handläggningen och organisationen finns i 104-117 kap. (avdelning H)." ;
    rdfs:label "Textpunkt"@sv .

inst:Stycke_inst:Paragraf_A_1_4_base_1 def:harPunkt inst:Punkt_inst:Stycke_inst:Paragraf_A_1_4_base_1_1 .
inst:Punkt_inst:Stycke_inst:Paragraf_A_1_4_base_1_1 def:ingårIStycke inst:Stycke_inst:Paragraf_A_1_4_base_1 .
inst:Punkt_inst:Stycke_inst:Paragraf_A_1_4_base_1_1 a def:Punkt ;
    def:harText "I avdelning A finns\n- allmänna bestämmelser, definitioner och förklaringar i 2 kap., och\n- övergripande bestämmelser om socialförsäkringsskyddet i 3-7 kap." ;
    rdfs:label "Textpunkt"@sv .

inst:Stycke_inst:Paragraf_A_1_4_base_2 def:harPunkt inst:Punkt_inst:Stycke_inst:Paragraf_A_1_4_base_2_1 .
inst:Punkt_inst:Stycke_inst:Paragraf_A_1_4_base_2_1 def:ingårIStycke inst:Stycke_inst:Paragraf_A_1_4_base_2 .
inst:Punkt_inst:Stycke_inst:Paragraf_A_1_4_base_2_1 a def:Punkt ;
    def:harText "Det finns definitioner och förklaringar också i andra avdelningar." ;
    rdfs:label "Textpunkt"@sv .

inst:Stycke_inst:Paragraf_A_2_10_base_1 def:harPunkt inst:Punkt_inst:Stycke_inst:Paragraf_A_2_10_base_1_1 .
