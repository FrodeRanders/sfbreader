inst:Punkt_inst:Stycke_inst:Paragraf_C_35_13_Upph_r_att_g_lla_U:2025-12-01_2_1 def:ingårIStycke inst:Stycke_inst:Paragraf_C_35_13_Upph_r_att_g_lla_U:2025-12-01_2 .
inst:Punkt_inst:Stycke_inst:Paragraf_C_35_13_Upph_r_att_g_lla_U:2025-12-01_2_1 a def:Punkt ;
    def:harText "Som framtida försäkringstid räknas en så stor andel av tiden från och med året för försäkringsfallet, till och med det år då den försäkrade fyller 65 år, som motsvarar kvoten mellan den faktiska försäkringstiden och fyra femtedelar av tiden från och med det år då den försäkrade fyllde 16 år till och med året före försäkringsfallet." ;
    rdfs:label "Textpunkt"@sv .

inst:Stycke_inst:Paragraf_C_35_14_base_1 def:harPunkt inst:Punkt_inst:Stycke_inst:Paragraf_C_35_14_base_1_1 .
