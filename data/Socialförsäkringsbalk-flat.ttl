    rdfs:label "Textpunkt"@sv .

inst:Stycke_inst:Paragraf_H_1_2022:1854_base_1 def:harPunkt inst:Punkt_inst:Stycke_inst:Paragraf_H_1_2022:1854_base_1_1 .
