inst:Stycke_inst:Paragraf_C_37_19_base_1 def:harPunkt inst:Punkt_inst:Stycke_inst:Paragraf_C_37_19_base_1_1 .
inst:Punkt_inst:Stycke_inst:Paragraf_C_37_19_base_1_1 def:ingårIStycke inst:Stycke_inst:Paragraf_C_37_19_base_1 .
