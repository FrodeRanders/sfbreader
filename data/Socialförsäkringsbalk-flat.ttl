    def:harText "Har upphävts genom" ;
    rdfs:label "Textpunkt"@sv .

inst:Stycke_inst:Paragraf_E_64_18_base_1 def:harPunkt inst:Punkt_inst:Stycke_inst:Paragraf_E_64_18_base_1_1 .
inst:Punkt_inst:Stycke_inst:Paragraf_E_64_18_base_1_1 def:ingårIStycke inst:Stycke_inst:Paragraf_E_64_18_base_1 .
inst:Punkt_inst:Stycke_inst:Paragraf_E_64_18_base_1_1 a def:Punkt ;
    def:harText "När pensionsrätt för premiepension har fastställts för en pensionssparare, ska Pensionsmyndigheten föra över medel som motsvarar pensionsrätten samt avkastningen på dessa medel till förvaltning i\n1. fonder som omfattas av fondavtal, eller\n2. fonder som förvaltas av Sjunde AP-fonden enligt lagen (2000:192) om allmänna pensionsfonder (AP-fonder)." ;
    rdfs:label "Textpunkt"@sv .

inst:Stycke_inst:Paragraf_E_64_18_base_2 def:harPunkt inst:Punkt_inst:Stycke_inst:Paragraf_E_64_18_base_2_1 .
inst:Punkt_inst:Stycke_inst:Paragraf_E_64_18_base_2_1 def:ingårIStycke inst:Stycke_inst:Paragraf_E_64_18_base_2 .
inst:Punkt_inst:Stycke_inst:Paragraf_E_64_18_base_2_1 a def:Punkt ;
    def:harText "Om pensionsspararen har tagit ut premiepension i form av livränta enligt 11-14 §§ ska medlen i stället föras över till Pensionsmyndighetens livränteverksamhet." ;
    rdfs:label "Textpunkt"@sv .

inst:Stycke_inst:Paragraf_E_64_19_base_1 def:harPunkt inst:Punkt_inst:Stycke_inst:Paragraf_E_64_19_base_1_1 .
inst:Punkt_inst:Stycke_inst:Paragraf_E_64_19_base_1_1 def:ingårIStycke inst:Stycke_inst:Paragraf_E_64_19_base_1 .
inst:Punkt_inst:Stycke_inst:Paragraf_E_64_19_base_1_1 a def:Punkt ;
