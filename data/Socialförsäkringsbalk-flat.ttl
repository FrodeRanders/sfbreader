inst:Punkt_inst:Stycke_inst:Paragraf_C_35_20_base_1_1 a def:Punkt ;
    def:harText "För den som inte kan tillgodoräknas 40 års försäkringstid gäller följande:" ;
    rdfs:label "Textpunkt"@sv .

inst:Stycke_inst:Paragraf_C_35_20_base_2 def:harPunkt inst:Punkt_inst:Stycke_inst:Paragraf_C_35_20_base_2_1 .
