inst:Punkt_inst:Stycke_inst:Paragraf_E_56_5_Upph_r_att_g_lla_U:2025-12-01_2_1 a def:Punkt ;
    def:harText "Bestämmelserna i första stycket tillämpas bara i fråga om pension som lämnas tidigast från och med den månad då den försäkrade fyller 66 år." ;
    rdfs:label "Textpunkt"@sv .

inst:Stycke_inst:Paragraf_E_56_6_base_1 def:harPunkt inst:Punkt_inst:Stycke_inst:Paragraf_E_56_6_base_1_1 .
inst:Punkt_inst:Stycke_inst:Paragraf_E_56_6_base_1_1 def:ingårIStycke inst:Stycke_inst:Paragraf_E_56_6_base_1 .
inst:Punkt_inst:Stycke_inst:Paragraf_E_56_6_base_1_1 a def:Punkt ;
    def:harText "En försäkrad som är född något av åren 1938-1953 och som får inkomstpension utan att ha rätt till tilläggspension ska, utan att behöva ansöka särskilt, få tilläggspension från och med den månad då han eller hon har fått rätt till sådan pension." ;
    rdfs:label "Textpunkt"@sv .

inst:Stycke_inst:Paragraf_E_56_7_base_1 def:harPunkt inst:Punkt_inst:Stycke_inst:Paragraf_E_56_7_base_1_1 .
inst:Punkt_inst:Stycke_inst:Paragraf_E_56_7_base_1_1 def:ingårIStycke inst:Stycke_inst:Paragraf_E_56_7_base_1 .
inst:Punkt_inst:Stycke_inst:Paragraf_E_56_7_base_1_1 a def:Punkt ;
    def:harText "Allmän ålderspension lämnas till och med den månad då rätten till pensionen upphör." ;
    rdfs:label "Textpunkt"@sv .

inst:Stycke_inst:Paragraf_E_56_8_Tr_der_i_kraft_I:2025-12-01_1 def:harPunkt inst:Punkt_inst:Stycke_inst:Paragraf_E_56_8_Tr_der_i_kraft_I:2025-12-01_1_1 .
inst:Punkt_inst:Stycke_inst:Paragraf_E_56_8_Tr_der_i_kraft_I:2025-12-01_1_1 def:ingårIStycke inst:Stycke_inst:Paragraf_E_56_8_Tr_der_i_kraft_I:2025-12-01_1 .
inst:Punkt_inst:Stycke_inst:Paragraf_E_56_8_Tr_der_i_kraft_I:2025-12-01_1_1 a def:Punkt ;
    def:harText "Pensionsmyndigheten ska utreda om den försäkrade vill ta ut allmän ålderspension om han eller hon inte har ansökt om sådan pension månaden före den då han eller hon uppnår riktåldern för pension och för den månaden har fått sjukersättning. Detta gäller inte i de fall 4 a § tillämpas." ;
    rdfs:label "Textpunkt"@sv .

inst:Stycke_inst:Paragraf_E_56_8_Upph_r_att_g_lla_U:2025-12-01_1 def:harPunkt inst:Punkt_inst:Stycke_inst:Paragraf_E_56_8_Upph_r_att_g_lla_U:2025-12-01_1_1 .
inst:Punkt_inst:Stycke_inst:Paragraf_E_56_8_Upph_r_att_g_lla_U:2025-12-01_1_1 def:ingårIStycke inst:Stycke_inst:Paragraf_E_56_8_Upph_r_att_g_lla_U:2025-12-01_1 .
inst:Punkt_inst:Stycke_inst:Paragraf_E_56_8_Upph_r_att_g_lla_U:2025-12-01_1_1 a def:Punkt ;
    def:harText "Pensionsmyndigheten ska utreda om den försäkrade vill ta ut allmän ålderspension om han eller hon inte har ansökt om sådan pension månaden före den då han eller hon fyller 66 år och för den månaden har fått sjukersättning. Detta gäller inte i de fall 4 a § tillämpas." ;
    rdfs:label "Textpunkt"@sv .

inst:Stycke_inst:Paragraf_E_56_9_base_1 def:harPunkt inst:Punkt_inst:Stycke_inst:Paragraf_E_56_9_base_1_1 .
inst:Punkt_inst:Stycke_inst:Paragraf_E_56_9_base_1_1 def:ingårIStycke inst:Stycke_inst:Paragraf_E_56_9_base_1 .
inst:Punkt_inst:Stycke_inst:Paragraf_E_56_9_base_1_1 a def:Punkt ;
    def:harText "Det som föreskrivs i 4 och 6 §§ gäller också i fråga om ökat uttag av allmän ålderspension." ;
    rdfs:label "Textpunkt"@sv .

inst:Stycke_inst:Paragraf_E_57_1_base_1 def:harPunkt inst:Punkt_inst:Stycke_inst:Paragraf_E_57_1_base_1_1 .
inst:Punkt_inst:Stycke_inst:Paragraf_E_57_1_base_1_1 def:ingårIStycke inst:Stycke_inst:Paragraf_E_57_1_base_1 .
