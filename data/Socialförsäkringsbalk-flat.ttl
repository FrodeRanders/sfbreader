    def:harText "Bilstöd lämnas i fall som avses i 3 § till en försäkrad som har barn som har ett sådant funktionshinder som avses i 2 §." ;
    rdfs:label "Textpunkt"@sv .

inst:Stycke_inst:Paragraf_D_52_11_base_1 def:harPunkt inst:Punkt_inst:Stycke_inst:Paragraf_D_52_11_base_1_1 .
inst:Punkt_inst:Stycke_inst:Paragraf_D_52_11_base_1_1 def:ingårIStycke inst:Stycke_inst:Paragraf_D_52_11_base_1 .
inst:Punkt_inst:Stycke_inst:Paragraf_D_52_11_base_1_1 a def:Punkt ;
    def:harText "Bilstöd enligt 10 § första stycket 4 och andra stycket lämnas under förutsättning att föräldern\n1. sammanbor med barnet, och\n2. behöver ett fordon för att förflytta sig tillsammans med barnet." ;
    rdfs:label "Textpunkt"@sv .

inst:Stycke_inst:Paragraf_D_52_12_base_1 def:harPunkt inst:Punkt_inst:Stycke_inst:Paragraf_D_52_12_base_1_1 .
inst:Punkt_inst:Stycke_inst:Paragraf_D_52_12_base_1_1 def:ingårIStycke inst:Stycke_inst:Paragraf_D_52_12_base_1 .
