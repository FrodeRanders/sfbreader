    rdfs:label "Textpunkt"@sv .

inst:Stycke_inst:Paragraf_H_1_2022:1266_base_1 def:harPunkt inst:Punkt_inst:Stycke_inst:Paragraf_H_1_2022:1266_base_1_1 .
inst:Punkt_inst:Stycke_inst:Paragraf_H_1_2022:1266_base_1_1 def:ingårIStycke inst:Stycke_inst:Paragraf_H_1_2022:1266_base_1 .
