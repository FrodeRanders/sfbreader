    rdfs:label "Textpunkt"@sv .

inst:Stycke_inst:Paragraf_G_102_29_Tr_der_i_kraft_I:2025-12-01_1 def:harPunkt inst:Punkt_inst:Stycke_inst:Paragraf_G_102_29_Tr_der_i_kraft_I:2025-12-01_1_1 .
