    def:harText "För garantipension finns särskilda bestämmelser om förmånstid i 66 och 67 kap." ;
    rdfs:label "Textpunkt"@sv .

inst:Stycke_inst:Paragraf_E_56_4_base_1 def:harPunkt inst:Punkt_inst:Stycke_inst:Paragraf_E_56_4_base_1_1 .
inst:Punkt_inst:Stycke_inst:Paragraf_E_56_4_base_1_1 def:ingårIStycke inst:Stycke_inst:Paragraf_E_56_4_base_1 .
inst:Punkt_inst:Stycke_inst:Paragraf_E_56_4_base_1_1 a def:Punkt ;
    def:harText "Allmän ålderspension lämnas från och med den månad som anges i ansökan, dock tidigast från och med den månad ansökan kom in till Pensionsmyndigheten." ;
    rdfs:label "Textpunkt"@sv .

inst:Stycke_inst:Paragraf_E_56_4a_Tr_der_i_kraft_I:2025-12-01_1 def:harPunkt inst:Punkt_inst:Stycke_inst:Paragraf_E_56_4a_Tr_der_i_kraft_I:2025-12-01_1_1 .
inst:Punkt_inst:Stycke_inst:Paragraf_E_56_4a_Tr_der_i_kraft_I:2025-12-01_1_1 def:ingårIStycke inst:Stycke_inst:Paragraf_E_56_4a_Tr_der_i_kraft_I:2025-12-01_1 .
inst:Punkt_inst:Stycke_inst:Paragraf_E_56_4a_Tr_der_i_kraft_I:2025-12-01_1_1 a def:Punkt ;
    def:harText "Allmän ålderspension får lämnas utan ansökan från och med den månad den försäkrade uppnår riktåldern för pension om den försäkrade fick hel sjukersättning omedelbart före den månaden." ;
    rdfs:label "Textpunkt"@sv .

inst:Stycke_inst:Paragraf_E_56_4a_Upph_r_att_g_lla_U:2025-12-01_1 def:harPunkt inst:Punkt_inst:Stycke_inst:Paragraf_E_56_4a_Upph_r_att_g_lla_U:2025-12-01_1_1 .
