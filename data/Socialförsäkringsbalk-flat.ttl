inst:Punkt_inst:Stycke_inst:Paragraf_D_52_15_base_2_1 a def:Punkt ;
    def:harText "Vid anskaffning av motorcykel eller moped lämnas dock grundbidrag med högst 12 000 respektive 3 000 kronor." ;
    rdfs:label "Textpunkt"@sv .

inst:Stycke_inst:Paragraf_D_52_16_base_1 def:harPunkt inst:Punkt_inst:Stycke_inst:Paragraf_D_52_16_base_1_1 .
inst:Punkt_inst:Stycke_inst:Paragraf_D_52_16_base_1_1 def:ingårIStycke inst:Stycke_inst:Paragraf_D_52_16_base_1 .
inst:Punkt_inst:Stycke_inst:Paragraf_D_52_16_base_1_1 a def:Punkt ;
    def:harText "Anskaffningsbidrag lämnas med högst 40 000 kronor." ;
    rdfs:label "Textpunkt"@sv .

inst:Stycke_inst:Paragraf_D_52_16_base_2 def:harPunkt inst:Punkt_inst:Stycke_inst:Paragraf_D_52_16_base_2_1 .
