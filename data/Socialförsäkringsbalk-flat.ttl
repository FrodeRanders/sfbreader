inst:Punkt_inst:Stycke_inst:Paragraf_D_52_16_base_2_1 a def:Punkt ;
    def:harText "Helt sådant bidrag lämnas till den vars årliga bruttoinkomst understiger 121 000 kronor." ;
    rdfs:label "Textpunkt"@sv .

inst:Stycke_inst:Paragraf_D_52_17_base_1 def:harPunkt inst:Punkt_inst:Stycke_inst:Paragraf_D_52_17_base_1_1 .
inst:Punkt_inst:Stycke_inst:Paragraf_D_52_17_base_1_1 def:ingårIStycke inst:Stycke_inst:Paragraf_D_52_17_base_1 .
inst:Punkt_inst:Stycke_inst:Paragraf_D_52_17_base_1_1 a def:Punkt ;
    def:harText "Till den vars årliga bruttoinkomst uppgår till 121 000 kronor eller mer, dock högst 210 000 kronor, lämnas anskaffningsbidrag med högst det belopp som återstår efter det att det belopp som anges i 16 § första stycket har reducerats med 400 kronor för varje tusental kronor som inkomsten överstiger 120 000 kronor." ;
    rdfs:label "Textpunkt"@sv .

inst:Stycke_inst:Paragraf_D_52_17_base_2 def:harPunkt inst:Punkt_inst:Stycke_inst:Paragraf_D_52_17_base_2_1 .
inst:Punkt_inst:Stycke_inst:Paragraf_D_52_17_base_2_1 def:ingårIStycke inst:Stycke_inst:Paragraf_D_52_17_base_2 .
inst:Punkt_inst:Stycke_inst:Paragraf_D_52_17_base_2_1 a def:Punkt ;
    def:harText "Till den vars årliga bruttoinkomst överstiger 210 000 kronor men inte 220 000 kronor lämnas anskaffningsbidrag med högst 4 000 kronor." ;
    rdfs:label "Textpunkt"@sv .

inst:Stycke_inst:Paragraf_D_52_17_base_3 def:harPunkt inst:Punkt_inst:Stycke_inst:Paragraf_D_52_17_base_3_1 .
inst:Punkt_inst:Stycke_inst:Paragraf_D_52_17_base_3_1 def:ingårIStycke inst:Stycke_inst:Paragraf_D_52_17_base_3 .
inst:Punkt_inst:Stycke_inst:Paragraf_D_52_17_base_3_1 a def:Punkt ;
    def:harText "Överstiger den årliga bruttoinkomsten 220 000 kronor lämnas inte något anskaffningsbidrag." ;
    rdfs:label "Textpunkt"@sv .

inst:Stycke_inst:Paragraf_D_52_18_base_1 def:harPunkt inst:Punkt_inst:Stycke_inst:Paragraf_D_52_18_base_1_1 .
