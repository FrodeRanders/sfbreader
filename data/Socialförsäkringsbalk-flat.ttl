inst:Punkt_inst:Stycke_inst:Paragraf_C_35_20_base_2_1 a def:Punkt ;
    def:harText "Garantinivån enligt 18 eller 19 § ska avkortas till så stor andel av denna som svarar mot kvoten mellan - försäkringstiden enligt 5 § andra stycket och\n- 40." ;
    rdfs:label "Textpunkt"@sv .

inst:Stycke_inst:Paragraf_C_35_21_base_1 def:harPunkt inst:Punkt_inst:Stycke_inst:Paragraf_C_35_21_base_1_1 .
inst:Punkt_inst:Stycke_inst:Paragraf_C_35_21_base_1_1 def:ingårIStycke inst:Stycke_inst:Paragraf_C_35_21_base_1 .
inst:Punkt_inst:Stycke_inst:Paragraf_C_35_21_base_1_1 a def:Punkt ;
    def:harText "Till grund för beräkning av garantiersättning ska ligga den årliga inkomstrelaterade sjukersättning eller aktivitetsersättning enligt 34 kap. som den försäkrade har rätt till för det år som garantiersättningen avser, före minskning med livränta enligt 36 kap. 3-8 §§." ;
    rdfs:label "Textpunkt"@sv .

inst:Stycke_inst:Paragraf_C_35_22_base_1 def:harPunkt inst:Punkt_inst:Stycke_inst:Paragraf_C_35_22_base_1_1 .
inst:Punkt_inst:Stycke_inst:Paragraf_C_35_22_base_1_1 def:ingårIStycke inst:Stycke_inst:Paragraf_C_35_22_base_1 .
inst:Punkt_inst:Stycke_inst:Paragraf_C_35_22_base_1_1 a def:Punkt ;
    def:harText "Med inkomstrelaterad sjukersättning och aktivitetsersättning avses i detta kapitel även sådana utländska förmåner som\n- ska avräknas från inkomstrelaterad ersättning enligt 34 kap. 14 §, och\n- inte är att likställa med garantiersättning enligt detta kapitel." ;
    rdfs:label "Textpunkt"@sv .

inst:Stycke_inst:Paragraf_C_35_23_base_1 def:harPunkt inst:Punkt_inst:Stycke_inst:Paragraf_C_35_23_base_1_1 .
inst:Punkt_inst:Stycke_inst:Paragraf_C_35_23_base_1_1 def:ingårIStycke inst:Stycke_inst:Paragraf_C_35_23_base_1 .
inst:Punkt_inst:Stycke_inst:Paragraf_C_35_23_base_1_1 a def:Punkt ;
    def:harText "För den som inte har inkomstrelaterad sjukersättning eller aktivitetsersättning motsvarar årlig hel garantiersättning garantinivån." ;
    rdfs:label "Textpunkt"@sv .

inst:Stycke_inst:Paragraf_C_35_24_base_1 def:harPunkt inst:Punkt_inst:Stycke_inst:Paragraf_C_35_24_base_1_1 .
