inst:Stycke_inst:Paragraf_E_64_17d_base_1 def:harPunkt inst:Punkt_inst:Stycke_inst:Paragraf_E_64_17d_base_1_1 .
inst:Punkt_inst:Stycke_inst:Paragraf_E_64_17d_base_1_1 def:ingårIStycke inst:Stycke_inst:Paragraf_E_64_17d_base_1 .
