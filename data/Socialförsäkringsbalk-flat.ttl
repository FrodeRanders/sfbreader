    def:harText "Äldreförsörjningsstöd kan lämnas tidigast från och med den månad då den försäkrade uppnår riktåldern för pension." ;
    rdfs:label "Textpunkt"@sv .

inst:Stycke_inst:Paragraf_E_74_7_Tr_der_i_kraft_I:2025-12-01_2 def:harPunkt inst:Punkt_inst:Stycke_inst:Paragraf_E_74_7_Tr_der_i_kraft_I:2025-12-01_2_1 .
