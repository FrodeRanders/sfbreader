    def:harText "Äldreförsörjningsstöd lämnas från och med den månad då rätt till förmånen har inträtt, dock tidigast från och med ansökningsmånaden." ;
    rdfs:label "Textpunkt"@sv .

inst:Stycke_inst:Paragraf_E_74_7_Upph_r_att_g_lla_U:2025-12-01_1 def:harPunkt inst:Punkt_inst:Stycke_inst:Paragraf_E_74_7_Upph_r_att_g_lla_U:2025-12-01_1_1 .
inst:Punkt_inst:Stycke_inst:Paragraf_E_74_7_Upph_r_att_g_lla_U:2025-12-01_1_1 def:ingårIStycke inst:Stycke_inst:Paragraf_E_74_7_Upph_r_att_g_lla_U:2025-12-01_1 .
